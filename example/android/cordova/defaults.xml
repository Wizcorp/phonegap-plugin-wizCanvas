<?xml version='1.0' encoding='utf-8'?>
<widget id="jp.wizcorp.exampleWizCanvas" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>exampleWizCanvas</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <feature name="WizCanvasPlugin">
        <param name="android-package" value="jp.wizcorp.phonegap.plugin.wizCanvas.WizCanvasPlugin" />
    </feature>
</widget>
